{
  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    // "moduleResolution": "nodenext",

    "paths": {
      "@interface/*": ["./src/app/lib/interface/*"],
      "@type/*": ["./src/app/lib/type/*"]
    }
  }
}
